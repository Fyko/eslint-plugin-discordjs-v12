{
    "name": "eslint-plugin-discordjs-v12",
    "version": "0.4.0",
    "description": "A plugin to help you update from Discord.js v11 to v12.",
    "author": "Carter Himmel",
    "main": "dist/src/index.js",
    "license": "Apache-2.0",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "files": [
        "/dist"
    ],
    "scripts": {
        "lint": "eslint src --ext .ts",
        "lint:fix": "eslint src --fix --ext .ts",
        "cleardir": "rimraf dist/*",
        "compile": "tsc",
        "prepublishOnly": "yarn build && yarn update-md",
        "build": "yarn cleardir && yarn compile",
        "test": "ava --verbose test/index.ts",
        "update-md": "inject-in-tag ./scripts/update-readme.js README.md"
    },
    "devDependencies": {
        "@ava/typescript": "^1.1.1",
        "@commitlint/cli": "^9.1.2",
        "@commitlint/config-angular": "^9.1.2",
        "@types/eslint": "^7.2.2",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "@typescript-eslint/typescript-estree": "^4.1.1",
        "ava": "^3.12.1",
        "babel-eslint": "^10.1.0",
        "create-eslint-index": "^1.0.0",
        "eslint": "^7.9.0",
        "eslint-ava-rule-tester": "^4.0.0",
        "eslint-config-marine": "^7",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.2.5",
        "import-modules": "^2.0.0",
        "inject-in-tag": "^1.1.1",
        "lint-staged": "^10.2.13",
        "prettier": "^2",
        "rimraf": "^3.0.2",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    },
    "eslintConfig": {
        "extends": "marine/prettier/node"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pnpm run update-md",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-angular"
        ],
        "rules": {
            "type-enum": [
                2,
                "always",
                [
                    "chore",
                    "build",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test",
                    "types",
                    "wip"
                ]
            ]
        }
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
